# the 2x2 case
T<-100
G<-2
N<-10
df<-tibble(id=rep(1:(N*G),T),          # id variable 1 2 3 ... 1 2 3
t=rep(1:100,each=20))%>%    # time variable 1 1 1 1 .... 2 2 2
mutate(D=1*(id<=N),             # Treated: D==1
A=1*(t>floor(100/2)),    # After:   A==1
AxD=A*D,                 # Treated times After
y=rnorm(n=N*G*T,mean=AxD*3,sd=1))%>%  # outcome y
group_by(D,A)%>%
mutate(ybar=mean(y)) # line for means
#df for labels
df_label<-summarise(df,ybar=mean(y))%>%
mutate(label=paste("ybar:",round(ybar,digits=3)),
x=25+A*50,y=-1+2*D)
ggplot(df,aes(x=t,y=y,colour=as.factor(D)))+geom_point(alpha=0.15)+
geom_line(aes(x=t,y=ybar),size=2)+
theme_minimal()+
labs(colour="Treated")+
geom_text(df_label,mapping=aes(x=x,y=y,label=label,size=2))+
geom_text(aes(x=25,y=6,label="PRE"),color="grey")+
geom_text(aes(x=75,y=6,label="POST"),color="grey")+
geom_vline(xintercept=50, linetype="dashed", color="grey", size=1.5)
